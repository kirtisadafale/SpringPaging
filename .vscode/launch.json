        {
            "type": "java",
            "name": "Launch PagingApplication (prod)",
            "request": "launch",
            "mainClass": "com.page.example.paging.PagingApplication",
            "projectName": "paging",
            "vmArgs": "-Dspring.profiles.active=prod"
